{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'AdminFeedback',\n  data() {\n    return {\n      feedbacks: []\n    };\n  },\n  async created() {\n    await this.fetchFeedbacks();\n  },\n  methods: {\n    async fetchFeedbacks() {\n      try {\n        const response = await axios.get('/api/admin/feedback', {\n          withCredentials: true\n        });\n        this.feedbacks = response.data.feedbacks;\n      } catch (error) {\n        console.error('Error fetching feedbacks:', error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","feedbacks","created","fetchFeedbacks","methods","response","get","withCredentials","error","console"],"sources":["src/components/AdminFeedback.vue"],"sourcesContent":["<template>\n  <div>\n    <!-- 顶部栏 -->\n    <div class=\"top-bar\">\n      <div class=\"left\">\n        <router-link to=\"/admin-dashboard\" class=\"back-link\">返回</router-link>\n        <span>Admin Feedbacks</span>\n      </div>\n    </div>\n\n    <!-- 反馈展示 -->\n    <div>\n      <h2 v-if=\"feedbacks.length > 0\">用户反馈</h2>\n      <div v-if=\"feedbacks.length > 0\">\n        <ul>\n          <li v-for=\"feedback in feedbacks\" :key=\"feedback.id\" class=\"feedback-item\">\n            <p><strong>FEEDBACK ID:</strong> {{ feedback.id }}</p>\n            <p><strong>User ID:</strong> {{ feedback.user_id }}</p>\n            <p><strong>User NAME:</strong> {{ feedback.username }}</p>\n            <p><strong>Message:</strong> {{ feedback.message }}</p>\n            <p><strong>Timestamp:</strong> {{ new Date(feedback.timestamp).toLocaleString() }}</p>\n          </li>\n        </ul>\n      </div>\n      <div v-else>\n        <p>No feedbacks found.</p>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  name: 'AdminFeedback',\n  data() {\n    return {\n      feedbacks: []\n    };\n  },\n  async created() {\n    await this.fetchFeedbacks();\n  },\n  methods: {\n    async fetchFeedbacks() {\n      try {\n        const response = await axios.get('/api/admin/feedback', { withCredentials: true });\n        this.feedbacks = response.data.feedbacks;\n      } catch (error) {\n        console.error('Error fetching feedbacks:', error);\n      }\n    }\n  }\n};\n</script>\n\n<style scoped>\n/* 顶部栏样式 */\n.top-bar {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  background-color: #D6EAF8;\n  padding: 10px;\n  color: #1296db;\n  font-weight: bold;\n}\n\n.top-bar .left {\n  display: flex;\n  align-items: center;\n}\n\n.top-bar .left .back-link {\n  margin-right: 15px;\n  padding: 5px 10px;\n  background-color: #ffffff;\n  color: #1296db;\n  border-radius: 5px;\n  text-decoration: none;\n  transition: background-color 0.3s, color 0.3s;\n  font-weight: bold;\n}\n\n.top-bar .left .back-link:hover {\n  background-color: #1296db;\n  color: #ffffff;\n}\n\n/* 反馈展示样式 */\n.feedback-item {\n  margin-bottom: 20px;\n  background-color: #f9f9f9;\n  padding: 10px;\n  border-radius: 8px;\n}\n</style>\n"],"mappings":"AAgCA,OAAAA,KAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;IACA;EACA;EACA,MAAAC,QAAA;IACA,WAAAC,cAAA;EACA;EACAC,OAAA;IACA,MAAAD,eAAA;MACA;QACA,MAAAE,QAAA,SAAAP,KAAA,CAAAQ,GAAA;UAAAC,eAAA;QAAA;QACA,KAAAN,SAAA,GAAAI,QAAA,CAAAL,IAAA,CAAAC,SAAA;MACA,SAAAO,KAAA;QACAC,OAAA,CAAAD,KAAA,8BAAAA,KAAA;MACA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}