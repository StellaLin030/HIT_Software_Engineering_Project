{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\naxios.defaults.withCredentials = true;\nimport router from '@/router/index.js';\nexport default {\n  name: 'LoginComponent',\n  data() {\n    return {\n      username: '',\n      password: '',\n      message: ''\n    };\n  },\n  methods: {\n    async login() {\n      try {\n        const response = await axios.post('/api/login', {\n          username: this.username,\n          password: this.password\n        }, {\n          withCredentials: true\n        });\n        this.message = response.data.message;\n        if (response.data.role === 'admin') {\n          router.push({\n            name: 'AdminDashboard'\n          });\n        } else {\n          router.push({\n            name: 'UserDashboard'\n          });\n        }\n      } catch (error) {\n        this.message = error.response && error.response.data ? error.response.data.message : '登录失败，请稍后重试。';\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","defaults","withCredentials","router","name","data","username","password","message","methods","login","response","post","role","push","error"],"sources":["src/components/Login.vue"],"sourcesContent":["<template>\n  <div class=\"login-page\">\n    <div class=\"login-container\">\n      <h2 class=\"login-title\">欢迎回来！</h2>\n      <form @submit.prevent=\"login\" class=\"login-form\">\n        <div class=\"form-group\">\n          <label for=\"username\">用户名：</label>\n          <input type=\"text\" v-model=\"username\" class=\"input-field\" required>\n        </div>\n        <div class=\"form-group\">\n          <label for=\"password\">密码：</label>\n          <input type=\"password\" v-model=\"password\" class=\"input-field\" required>\n        </div>\n        <button type=\"submit\" class=\"login-button\">登录</button>\n      </form>\n      <p class=\"login-message\">{{ message }}</p>\n      <p>还没有账号？ <router-link to=\"/register\">点击这里注册</router-link></p>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\naxios.defaults.withCredentials = true;\nimport router from '@/router/index.js';\n\nexport default {\n  name: 'LoginComponent',\n  data() {\n    return {\n      username: '',\n      password: '',\n      message: ''\n    };\n  },\n  methods: {\n    async login() {\n      try {\n        const response = await axios.post('/api/login', {\n          username: this.username,\n          password: this.password\n        }, { withCredentials: true });\n        this.message = response.data.message;\n        if (response.data.role === 'admin') {\n          router.push({ name: 'AdminDashboard' });\n        } else {\n          router.push({ name: 'UserDashboard' });\n        }\n      } catch (error) {\n        this.message = error.response && error.response.data ? error.response.data.message : '登录失败，请稍后重试。';\n      }\n    }\n  }\n};\n</script>\n\n<style scoped>\n.login-page {\n  background-image: url('@/assets/homepage0.png');\n  background-size: cover;\n  background-position: center;\n  height: 100vh;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\n\n.login-container {\n  max-width: 400px;\n  width: 100%;\n  background-color: white;\n  padding: 20px;\n  border-radius: 10px;\n  box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);\n  text-align: center;\n}\n\n.login-title {\n  margin-bottom: 30px;\n  font-size: 28px;\n  font-weight: 600;\n}\n\n.form-group {\n  margin-bottom: 20px;\n  display: flex;\n  align-items: center;\n}\n\n.form-group label {\n  width: 80px;\n}\n\n.input-field {\n  width: 250px;\n  padding: 10px;\n  border: 1px solid #ccc;\n  border-radius: 5px;\n  box-shadow: 0 0 5px rgba(0, 0, 0, 0.1);\n  transition: box-shadow 0.3s ease;\n}\n\n.input-field:focus {\n  outline: none;\n  box-shadow: 0 0 5px rgba(0, 0, 0, 0.3);\n}\n\n.login-button {\n  background-color: #1296db;\n  color: #fff;\n  padding: 12px 24px;\n  border: none;\n  border-radius: 5px;\n  font-size: 18px;\n  font-weight: 600;\n  cursor: pointer;\n  transition: background-color 0.3s ease;\n}\n\n.login-button:hover {\n  background-color: #0056b3;\n}\n\n.login-message {\n  margin-top: 20px;\n  font-size: 16px;\n  color: #ff0000;\n}\n\na {\n  color: #007bff;\n  text-decoration: none;\n}\n\na:hover {\n  text-decoration: underline;\n}\n</style>\n"],"mappings":";AAsBA,OAAAA,KAAA;AACAA,KAAA,CAAAC,QAAA,CAAAC,eAAA;AACA,OAAAC,MAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,QAAA;MACAC,QAAA;MACAC,OAAA;IACA;EACA;EACAC,OAAA;IACA,MAAAC,MAAA;MACA;QACA,MAAAC,QAAA,SAAAX,KAAA,CAAAY,IAAA;UACAN,QAAA,OAAAA,QAAA;UACAC,QAAA,OAAAA;QACA;UAAAL,eAAA;QAAA;QACA,KAAAM,OAAA,GAAAG,QAAA,CAAAN,IAAA,CAAAG,OAAA;QACA,IAAAG,QAAA,CAAAN,IAAA,CAAAQ,IAAA;UACAV,MAAA,CAAAW,IAAA;YAAAV,IAAA;UAAA;QACA;UACAD,MAAA,CAAAW,IAAA;YAAAV,IAAA;UAAA;QACA;MACA,SAAAW,KAAA;QACA,KAAAP,OAAA,GAAAO,KAAA,CAAAJ,QAAA,IAAAI,KAAA,CAAAJ,QAAA,CAAAN,IAAA,GAAAU,KAAA,CAAAJ,QAAA,CAAAN,IAAA,CAAAG,OAAA;MACA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}