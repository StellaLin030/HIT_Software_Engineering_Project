{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nimport router from \"@/router\";\nexport default {\n  name: 'AdminDashboard',\n  data() {\n    return {\n      like: true,\n      value1: null,\n      value2: null,\n      value3: null,\n      title1: \"增长人数(较上月的今天)\",\n      title2: \"总人数\",\n      title3: \"总反馈数\",\n      unread_feedback: null,\n      logoutMessage: ''\n    };\n  },\n  methods: {\n    logout() {\n      router.push({\n        name: 'Logout'\n      });\n    },\n    goToFeedback() {\n      router.push({\n        name: 'AdminFeedback'\n      });\n    },\n    goToUserInformation() {\n      router.push({\n        name: 'AdminUserInfo'\n      });\n    },\n    goToOverallSituation() {\n      router.push({\n        name: 'AdminOverallSituation'\n      });\n    },\n    goToCreaseUserInfo() {\n      router.push({\n        name: 'UserCreaseInfo'\n      });\n    },\n    goToUnreadFeedback() {\n      router.push({\n        name: 'AdminUnreadFeedback'\n      });\n    },\n    // 获取总人数\n    fetchUserCount() {\n      axios.get('/api/admin/number_user', {\n        withCredentials: true\n      }).then(response => {\n        this.value2 = response.data.total_users;\n      }).catch(error => {\n        console.error(\"There was an error fetching the user count:\", error);\n      });\n    },\n    // 获取增长人数\n    fetchIncreaseUserCount() {\n      axios.get('/api/admin/increase_user', {\n        withCredentials: true\n      }).then(response => {\n        this.value1 = response.data.increase_users;\n      }).catch(error => {\n        console.error(\"There was an error fetching the increase user count:\", error);\n      });\n    },\n    // 获取未读反馈数量\n    fetchUnreadFeedbackCount() {\n      axios.get('/api/admin/number_unreadfeedback', {\n        withCredentials: true\n      }).then(response => {\n        this.unread_feedback = response.data.unread_feedback;\n      }).catch(error => {\n        console.error(\"There was an error fetching the unread feedback count:\", error);\n      });\n    },\n    // 获取未读反馈数量\n    fetchAllFeedbackCount() {\n      axios.get('/api/admin/number_allfeedback', {\n        withCredentials: true\n      }).then(response => {\n        this.value3 = response.data.allfeedback;\n      }).catch(error => {\n        console.error(\"There was an error fetching the all feedback count:\", error);\n      });\n    }\n  },\n  // 钩子 处于该界面自动获取\n  created() {\n    this.fetchUserCount();\n    this.fetchIncreaseUserCount();\n    this.fetchUnreadFeedbackCount();\n    this.fetchAllFeedbackCount();\n  }\n};","map":{"version":3,"names":["axios","router","name","data","like","value1","value2","value3","title1","title2","title3","unread_feedback","logoutMessage","methods","logout","push","goToFeedback","goToUserInformation","goToOverallSituation","goToCreaseUserInfo","goToUnreadFeedback","fetchUserCount","get","withCredentials","then","response","total_users","catch","error","console","fetchIncreaseUserCount","increase_users","fetchUnreadFeedbackCount","fetchAllFeedbackCount","allfeedback","created"],"sources":["src/components/AdminDashboard.vue"],"sourcesContent":["<template>\r\n  <div class=\"admin-dashboard-background\">\r\n    <!-- 顶部栏 -->\r\n    <div class=\"top-bar\">\r\n      <div class=\"left\">\r\n        <span>ChatWithAIs V1 | 一站式大语言模型访问平台</span>\r\n      </div>\r\n      <div class=\"right\">\r\n        <button @click=\"logout\" class=\"logout-button\">登出</button>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 统计信息 -->\r\n    <div class=\"dashboard-container\">\r\n      <h2>管理员仪表板</h2>\r\n      <el-row :gutter=\"20\">\r\n        <el-col :span=\"6\">\r\n          <div>\r\n            <el-statistic\r\n                group-separator=\",\"\r\n                :precision=\"0\"\r\n                :value=\"value2\"\r\n                :title=\"title2\"\r\n            >\r\n              <template slot=\"suffix\">\r\n                <span @click=\"goToUserInformation\" class=\"like\">\r\n                  <i\r\n                      class=\"el-icon-view\"\r\n                      style=\"color:red\"\r\n                  ></i>\r\n                </span>\r\n              </template>\r\n            </el-statistic>\r\n          </div>\r\n        </el-col>\r\n        <el-col :span=\"6\">\r\n          <div>\r\n            <el-statistic\r\n                group-separator=\",\"\r\n                :precision=\"0\"\r\n                decimal-separator=\".\"\r\n                :value=\"value1\"\r\n                :title=\"title1\"\r\n            >\r\n              <template slot=\"prefix\">\r\n                <i @click=\"goToCreaseUserInfo\" class=\"el-icon-s-flag\" style=\"color: red\"></i>\r\n              </template>\r\n              <template slot=\"suffix\">\r\n                <i @click=\"goToCreaseUserInfo\" class=\"el-icon-s-flag\" style=\"color: blue\"></i>\r\n              </template>\r\n            </el-statistic>\r\n          </div>\r\n        </el-col>\r\n        <el-col :span=\"6\">\r\n          <div>\r\n            <el-statistic :value=unread_feedback title=\"未读反馈\">\r\n              <template slot=\"suffix\">\r\n                <span @click=\"goToUnreadFeedback\" class=\"like\">\r\n                  <i\r\n                      class=\"el-icon-view\"\r\n                      style=\"color:red\"\r\n                  ></i>\r\n                </span>\r\n              </template>\r\n            </el-statistic>\r\n          </div>\r\n        </el-col>\r\n        <el-col :span=\"6\">\r\n          <div>\r\n            <el-statistic\r\n                group-separator=\",\"\r\n                :precision=\"0\"\r\n                :value=\"value3\"\r\n                :title=\"title3\"\r\n            >\r\n              <template slot=\"suffix\">\r\n                <span @click=\"goToFeedback\" class=\"like\">\r\n                  <i\r\n                      class=\"el-icon-view\"\r\n                      style=\"color:red\"\r\n                  ></i>\r\n                </span>\r\n              </template>\r\n            </el-statistic>\r\n          </div>\r\n        </el-col>\r\n      </el-row>\r\n      <p>{{ logoutMessage }}</p>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport router from \"@/router\";\r\n\r\nexport default {\r\n  name: 'AdminDashboard',\r\n  data() {\r\n    return {\r\n      like: true,\r\n      value1: null,\r\n      value2: null,\r\n      value3: null,\r\n      title1: \"增长人数(较上月的今天)\",\r\n      title2: \"总人数\",\r\n      title3: \"总反馈数\",\r\n      unread_feedback:null,\r\n      logoutMessage: ''\r\n    };\r\n  },\r\n  methods: {\r\n    logout() {\r\n      router.push({ name: 'Logout' });\r\n    },\r\n    goToFeedback() {\r\n      router.push({ name: 'AdminFeedback' });\r\n    },\r\n    goToUserInformation() {\r\n      router.push({ name: 'AdminUserInfo' });\r\n    },\r\n    goToOverallSituation(){\r\n      router.push({ name: 'AdminOverallSituation' });\r\n    },\r\n    goToCreaseUserInfo() {\r\n      router.push({ name: 'UserCreaseInfo' });\r\n    },\r\n    goToUnreadFeedback() {\r\n      router.push({ name: 'AdminUnreadFeedback' });\r\n    },\r\n    // 获取总人数\r\n    fetchUserCount() {\r\n      axios.get('/api/admin/number_user', { withCredentials: true })\r\n          .then(response => {\r\n            this.value2 = response.data.total_users;\r\n          })\r\n          .catch(error => {\r\n            console.error(\"There was an error fetching the user count:\", error);\r\n          });\r\n    },\r\n    // 获取增长人数\r\n    fetchIncreaseUserCount() {\r\n      axios.get('/api/admin/increase_user', { withCredentials: true })\r\n          .then(response => {\r\n            this.value1 = response.data.increase_users;\r\n          })\r\n          .catch(error => {\r\n            console.error(\"There was an error fetching the increase user count:\", error);\r\n          });\r\n    },\r\n    // 获取未读反馈数量\r\n    fetchUnreadFeedbackCount() {\r\n      axios.get('/api/admin/number_unreadfeedback', { withCredentials: true })\r\n          .then(response => {\r\n            this.unread_feedback = response.data.unread_feedback;\r\n          })\r\n          .catch(error => {\r\n            console.error(\"There was an error fetching the unread feedback count:\", error);\r\n          });\r\n    },\r\n    // 获取未读反馈数量\r\n    fetchAllFeedbackCount() {\r\n      axios.get('/api/admin/number_allfeedback', { withCredentials: true })\r\n          .then(response => {\r\n            this.value3 = response.data.allfeedback;\r\n          })\r\n          .catch(error => {\r\n            console.error(\"There was an error fetching the all feedback count:\", error);\r\n          });\r\n    },\r\n  },\r\n\r\n  // 钩子 处于该界面自动获取\r\n  created() {\r\n    this.fetchUserCount();\r\n    this.fetchIncreaseUserCount();\r\n    this.fetchUnreadFeedbackCount();\r\n    this.fetchAllFeedbackCount();\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.admin-dashboard-background {\r\n  background-image: url('@/assets/feedback.png');\r\n  background-size: cover;\r\n  background-position: center;\r\n  height: 100vh;\r\n}\r\n\r\n.top-bar {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  background-color: #D6EAF8;\r\n  padding: 10px;\r\n  color: #1296db;\r\n  height: 35px;\r\n  font-weight: bold;\r\n}\r\n\r\n.top-bar .left {\r\n  font-size: 20px;\r\n}\r\n\r\n.top-bar .right .logout-button {\r\n  margin-left: 15px;\r\n  padding: 8px 16px;\r\n  background-color: #ffffff;\r\n  color: #1296db;\r\n  border-radius: 5px;\r\n  text-decoration: none;\r\n  transition: background-color 0.3s, color 0.3s;\r\n  font-weight: bold;\r\n  border: none;\r\n  font-size: 15px;\r\n}\r\n\r\n.top-bar .right .logout-button:hover {\r\n  background-color: #1296db;\r\n  color: #ffffff;\r\n}\r\n\r\n.dashboard-container {\r\n  position: absolute;\r\n  top: 50%;\r\n  left: 50%;\r\n  transform: translate(-50%, -50%);\r\n  text-align: center;\r\n  background-color: #fff;\r\n  padding: 20px;\r\n  border-radius: 10px;\r\n  width: 80%;\r\n}\r\n\r\n.function-buttons {\r\n  margin-top: 20px;\r\n}\r\n\r\n.dashboard-button {\r\n  margin: 10px;\r\n  padding: 10px 20px;\r\n  background-color: #1296db;\r\n  color: #fff;\r\n  border: none;\r\n  border-radius: 5px;\r\n  font-size: 16px;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s ease;\r\n}\r\n\r\n.dashboard-button:hover {\r\n  background-color: #0056b3;\r\n}\r\n\r\n.like {\r\n  cursor: pointer;\r\n  font-size: 25px;\r\n  display: inline-block;\r\n}\r\n</style>\r\n"],"mappings":";AA6FA,OAAAA,KAAA;AACA,OAAAC,MAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,IAAA;MACAC,MAAA;MACAC,MAAA;MACAC,MAAA;MACAC,MAAA;MACAC,MAAA;MACAC,MAAA;MACAC,eAAA;MACAC,aAAA;IACA;EACA;EACAC,OAAA;IACAC,OAAA;MACAb,MAAA,CAAAc,IAAA;QAAAb,IAAA;MAAA;IACA;IACAc,aAAA;MACAf,MAAA,CAAAc,IAAA;QAAAb,IAAA;MAAA;IACA;IACAe,oBAAA;MACAhB,MAAA,CAAAc,IAAA;QAAAb,IAAA;MAAA;IACA;IACAgB,qBAAA;MACAjB,MAAA,CAAAc,IAAA;QAAAb,IAAA;MAAA;IACA;IACAiB,mBAAA;MACAlB,MAAA,CAAAc,IAAA;QAAAb,IAAA;MAAA;IACA;IACAkB,mBAAA;MACAnB,MAAA,CAAAc,IAAA;QAAAb,IAAA;MAAA;IACA;IACA;IACAmB,eAAA;MACArB,KAAA,CAAAsB,GAAA;QAAAC,eAAA;MAAA,GACAC,IAAA,CAAAC,QAAA;QACA,KAAAnB,MAAA,GAAAmB,QAAA,CAAAtB,IAAA,CAAAuB,WAAA;MACA,GACAC,KAAA,CAAAC,KAAA;QACAC,OAAA,CAAAD,KAAA,gDAAAA,KAAA;MACA;IACA;IACA;IACAE,uBAAA;MACA9B,KAAA,CAAAsB,GAAA;QAAAC,eAAA;MAAA,GACAC,IAAA,CAAAC,QAAA;QACA,KAAApB,MAAA,GAAAoB,QAAA,CAAAtB,IAAA,CAAA4B,cAAA;MACA,GACAJ,KAAA,CAAAC,KAAA;QACAC,OAAA,CAAAD,KAAA,yDAAAA,KAAA;MACA;IACA;IACA;IACAI,yBAAA;MACAhC,KAAA,CAAAsB,GAAA;QAAAC,eAAA;MAAA,GACAC,IAAA,CAAAC,QAAA;QACA,KAAAd,eAAA,GAAAc,QAAA,CAAAtB,IAAA,CAAAQ,eAAA;MACA,GACAgB,KAAA,CAAAC,KAAA;QACAC,OAAA,CAAAD,KAAA,2DAAAA,KAAA;MACA;IACA;IACA;IACAK,sBAAA;MACAjC,KAAA,CAAAsB,GAAA;QAAAC,eAAA;MAAA,GACAC,IAAA,CAAAC,QAAA;QACA,KAAAlB,MAAA,GAAAkB,QAAA,CAAAtB,IAAA,CAAA+B,WAAA;MACA,GACAP,KAAA,CAAAC,KAAA;QACAC,OAAA,CAAAD,KAAA,wDAAAA,KAAA;MACA;IACA;EACA;EAEA;EACAO,QAAA;IACA,KAAAd,cAAA;IACA,KAAAS,sBAAA;IACA,KAAAE,wBAAA;IACA,KAAAC,qBAAA;EA